@model Hackathon.Feature.PageContent.Models.ViewModels.TeamListViewModel

@if (Model != null)
{ 
    <div id="fh5co-work-section">
        <div class="container">
            <div class="row">
                <div class="col-md-6 col-md-offset-3 text-center animate-box ">
                    <div class="heading-section">
                        <h2>@Model.Title</h2>
                    </div>
                </div>
            </div>
            <div class="row">

                @foreach (var team in Model.Teams)
                {
                    <div class="team-box col-md-6 text-center animate-box">
                        <a href="#" class="view">
                            <div class="work" >
                                <span class="team-name">@team.Name</span>
                            </div>
                        </a>
                    </div>
                }
                @*
        <pclass="text-center view-button animate-box"><a href="#" class="btn btn-primary btn-outline btn-lg">See More Project</a></p>
                *@
            </div>
        </div>
    </div>
}